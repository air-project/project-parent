	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>**/*.properties</include>
				</includes>
				<filtering>true</filtering>
			</resource>
			<resource>
				<directory>src/main/resources</directory>
			</resource>
		</resources>
		<filters>
			<filter>../props-${env}.properties</filter>
		</filters>
		<pluginManagement>
			<plugins>
				<!-- jaxb从schema生成对象 -->
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>jaxb2-maven-plugin</artifactId>
					<executions>
						<execution>
							<goals>
								<goal>xjc</goal>
							</goals>
						</execution>
					</executions>
					<configuration>
						<!-- The schema directory or xsd files. -->
						<schemaDirectory>${basedir}/src/test/resources/xsd/static_xsd/</schemaDirectory>
						<schemaFiles>currency.xsd</schemaFiles>
						<!-- The package in which the source files will be generated. -->
						<packageName>com.citsamex.kernel.eip.hotel.data.currency</packageName>
						<!-- The working directory to create the generated java source files. -->
						<outputDirectory>${project.build.directory}/generated-sources/jaxb</outputDirectory>
						<extension>true</extension>
						<args>
							<arg>-no-header</arg>
							<arg>-Xxew</arg>
							<arg>-Xxew:instantiate lazy</arg>
							<arg>-Xxew:delete</arg>
						</args>
						<plugins>
							<plugin>
								<groupId>com.github.jaxb-xew-plugin</groupId>
								<artifactId>jaxb-xew-plugin</artifactId>
								<version>1.0</version>
							</plugin>
						</plugins>
					</configuration>
				</plugin>
				<!-- 配置tomcat6咄1�7�件，可以在6或�1�7�1�7�测评1�7 -->
				<plugin>
					<groupId>org.apache.tomcat.maven</groupId>
					<artifactId>tomcat6-maven-plugin</artifactId>
					<version>${tomcat-maven-plugin-version}</version>
				</plugin>
				<plugin>
					<groupId>org.apache.tomcat.maven</groupId>
					<artifactId>tomcat7-maven-plugin</artifactId>
					<version>${tomcat-maven-plugin-version}</version>
				</plugin>
				<!-- 配置jetty -->
				<plugin>
					<groupId>org.mortbay.jetty</groupId>
					<artifactId>jetty-maven-plugin</artifactId>
					<version>${jetty-maven-plugin-version}</version>
					<configuration>
						<scanIntervalSeconds>0</scanIntervalSeconds>
						<stopKey>jetty-stop</stopKey>
						<stopPort>9999</stopPort>
						<webApp>
							<contextPath>/${project.artifactId}</contextPath>
						</webApp>
						<systemProperties>
							<!-- 监听端口 -->
							<systemProperty>
								<name>jetty.port</name>
								<value>${maven.jetty.port}</value>
							</systemProperty>
							<!-- 默认uri字符集处琄1�7 -->
							<systemProperty>
								<name>org.eclipse.jetty.util.URI.charset</name>
								<value>${maven.jetty.uriEncoding}</value>
							</systemProperty>
						</systemProperties>
					</configuration>
				</plugin>
				<!-- 编译环境覄1�76以上 -->
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>${maven-compiler-plugin-version}</version>
					<configuration>
						<source>${jdk-version}</source>
						<target>${jdk-version}</target>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.mybatis.generator</groupId>
					<artifactId>mybatis-generator-maven-plugin</artifactId>
					<version>${mybatis-generator-core}</version>
					<executions>
						<execution>
							<id>Generate MyBatis Artifacts</id>
							<goals>
								<goal>generate</goal>
							</goals>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>findbugs-maven-plugin</artifactId>
					<version>${findbugs.version}</version>
					<configuration>
						<xmlOutput>true</xmlOutput>
						<effort>Max</effort>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>dashboard-maven-plugin</artifactId>
					<version>${dashboard.version}</version>
				</plugin>
				<plugin>
					<groupId>org.apache.cxf</groupId>
					<artifactId>cxf-codegen-plugin</artifactId>
					<version>${cxf-version}</version>
				</plugin>
				<!-- YUI压缩插件 -->
				<plugin>
					<groupId>net.alchim31.maven</groupId>
					<artifactId>yuicompressor-maven-plugin</artifactId>
					<executions>
						<execution>
							<phase>compile</phase>
							<goals>
								<goal>compress</goal>
							</goals>
						</execution>
					</executions>
					<configuration>
						<encoding>UTF-8</encoding>
						<!-- 忽略 js 错误警告 -->
						<jswarn>false</jswarn>
						<nosuffix>true</nosuffix>
						<linebreakpos>-1</linebreakpos>
						<includes>
							<include>js/**/*.js</include>
							<include>css/**/*.css</include>
							<include>js/**/*.css</include>
						</includes>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
